<h1>Hello WOlrd</h1>


<% if user_signed_in? %>
<h1>HELLO</h1>
<%= link_to 'log out', destroy_user_session_url, method: :delete %>
<% else %>
<%= link_to 'log in', new_user_session_url %>
<% end %>

<% if customer_signed_in? %>
<%= link_to 'log out', destroy_customer_session_url, method: :delete %>
<% else %>
<%= link_to 'log in', new_customer_session_url %>
<% end %>

<h1>Kopi</h1>

<ul>
    <% @kopis.each do |kopi| %>
    <li><%= kopi.name %></li>
    <small><%= kopi.roast.roastedness %><small>
    <small><%= kopi.farm.name %><small>
    <% end %>
</ul>






